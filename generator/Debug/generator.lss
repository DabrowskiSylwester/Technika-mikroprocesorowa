000136 f409                      	brne cm_if_done 
000137 e5ba                      	ldi r27, 90
                                 cm_if_done:
000138 9b5a                      	sbis portd, 2 ; if it is pressed exit cm_loop
000139 c003                      	rjmp cm_done
