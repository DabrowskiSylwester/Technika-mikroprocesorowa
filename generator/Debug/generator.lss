                                 control_mode:
                                 ; Control mode is using 6 buttons:
                                 ; PD2 - enter/exit control mode
                                 ; PD3 - choose signal: PWM (display0: P), sine (S) or triangle (|-) 
                                 ; PD0 - increase frequency (display1-3)
                                 ; PD1 - decrease frequency (display1-3)
                                 ; PD4 - increase duty cycle 
                                 ; PD5 - decrease duty cycle [we can use diodes to display current duty cycle and change it 10:5:90]
                                 
00011b 94f8                      	cli ;diseble interrupts
                                 cm_loop:
00011c 940e 01d4                 	call display ; display current settings
00011e 940e 01d5                 	call LEDdriver
                                 	; check if button is pressed
                                 cm_signal:
000120 9b5b                      	sbis portd, 3 
000121 9593                      	inc r25
000122 3094                      	cpi r25, 4 ; if greater than 3 load 1 
000123 f409                      	brne cm_freq 
000124 e091                      	ldi r25, 1
                                 cm_freq:
000125 9b58                      	sbis portd, 0
000126 95a3                      	inc r26
000127 30a6                      	cpi r26, 6 ; if greater than 5 load 1
000128 f409                      	brne cm_freq_dec
